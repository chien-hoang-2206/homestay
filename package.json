    "main": "index.js",
    "type": "module",
    "scripts": {
        "test": "jest",
        "test:coverage": "jest --coverage",
        "backend": "nodemon backend/index.js",
        "frontend": "yarn --cwd frontend run dev",
        "start:backend": "pm2 start backend/index.js --name homestay_be --  --port 5000",
        "start:frontend": "pm2 start \"yarn --cwd frontend run dev\" --name homestay_fe",
        "start:all": "concurrently \"yarn start:backend\" \"yarn start:frontend\"",
        "restart:backend": "pm2 restart homestay_be",
        "restart:frontend": "pm2 restart homestay_fe",
        "restart:all": "pm2 restart all",
        "stop:backend": "pm2 stop homestay_be",
        "stop:frontend": "pm2 stop homestay_fe",
        "stop:all": "pm2 stop all",
        "logs:backend": "pm2 logs homestay_be",
        "logs:frontend": "pm2 logs homestay_fe",
        "logs:all": "pm2 logs",
        "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore"
    },
